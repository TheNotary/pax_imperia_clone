{
    "jest.nodeEnv": {
        "NODE_OPTIONS": "--experimental-vm-modules"
    },
    "jestTestExplorer.pathToJest": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "jest.autoClearTerminal": false,
